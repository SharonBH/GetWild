/* Tab Navigation */

.tab-content {
    margin-right: 3px;
}

.nav-tabs {
    margin: 0;
    padding: 0;
    border: 0;    
}
.nav-tabs > li > a {
    background: #DADADA;
    border-radius: 0;
    box-shadow: inset 0 -8px 7px -9px rgba(0,0,0,.2),-2px -2px 5px -2px rgba(0,0,0,.2);
}
.nav-tabs > li.active > a,
.nav-tabs > li.active > a:hover {
    background: #f0ffff;
    box-shadow: inset 0 0 0 0 rgba(0,0,0,.3),-2px -3px 5px -2px rgba(0,0,0,.3);
}

/* Tab Content */
.tab-pane {
    background: #fff;
    box-shadow: 0 0 4px rgba(0, 120, 255, 0.30);
    border-radius: 0;
    /*text-align: center;*/
    padding: 10px;
}


.jumbotron {

    padding: 1% !important;
    background-color: transparent;
}

.jumbotron img {
    width: 100%;
    max-height: 400px;
    border-radius: 25px;
}

.control-label {
    text-align: right !important;
}


.has-success .help-block,
.has-success .control-label {
  color: #233D7F;
}
.has-success .form-control {
  border-color: #233D7F;
  -webkit-box-shadow: inset 0 1px 1px rgba(0, 0, 0, 0.075);
  box-shadow: inset 0 1px 1px rgba(0, 0, 0, 0.075);
}
.has-success .form-control:focus {
  border-color: #233D7F;
  -webkit-box-shadow: inset 0 1px 1px rgba(0, 0, 0, 0.075), 0 0 6px #233D7F;
  box-shadow: inset 0 1px 1px rgba(0, 0, 0, 0.075), 0 0 6px #233D7F;
}

.panel-footer {
    background-color: #ED1C24 !important;
    border-top: none;
}




